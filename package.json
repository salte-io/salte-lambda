  },
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.23.0",
    "coveralls": "^3.0.0",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^4.0.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-ava": "^4.2.2",
    "husky": "^0.14.3",
    "nps": "^5.7.1",
    "nyc": "^11.3.0",
